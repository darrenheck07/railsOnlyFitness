<header>
  <%= render 'home/header' %>
</header>

<p id="notice"><%= notice %></p>

<div class = "container"> 

<div class = "row justify-content-center">
  <h1>Chest/Triceps 4 - Home</h1>
</div>

<table class = "table table-hover table-dark">
  <thead class = "thead-light">
    <tr>
      <th>Exercise</th>
      <th>Reps</th>
      <th>Sets</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chesttricepshome4s.each do |chesttricepshome4| %>
      <tr>
        <td style="color: #01a6ee;" ><%= chesttricepshome4.exercise %></td>
        <td style="color: #01a6ee;" ><%= chesttricepshome4.reps %></td>
        <td style="color: #01a6ee;" ><%= chesttricepshome4.sets %></td>
       <!--<td><%= link_to 'Show', chesttricepshome1 %></td> -->
        <td ><%= link_to 'Edit', edit_chesttricepshome4_path(chesttricepshome4), class: "btn btn-secondary", style: "color: #01a6ee" %></td>
        <td><%= link_to 'Destroy', chesttricepshome4, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-secondary", style: "color: #01a6ee" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chesttricepshome4', new_chesttricepshome4_path %> 

<br>
<br>

<div class = "row justify-content-center" >
  <iframe src="https://open.spotify.com/embed/playlist/37i9dQZF1DX0XUsuxWHRQd" width="500" height="380" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
</div>

</div>